<Grammar>

  <Node Type="SyntaxToken" Base="SyntaxNode">
    <Comment>
      <summary>
        Defines a record which represents a token in the syntax tree.
      </summary>
    </Comment>

    <Member Name="Text" Type="string">
      <Comment>
        <summary>
          Gets the original text for this token.
        </summary>
      </Comment>
    </Member>

    <Kind Name="BadToken" />

